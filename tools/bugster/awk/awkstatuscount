{
    count[$4]++
}
END {
    total = 0

    item="1-Dis"
    printf "%s %d\n", item, count[item]
    total += count[item]

    item="2-Inc"
    printf "%s %d\n", item, count[item]
    total += count[item]

    item="3-Acc"
    printf "%s %d\n", item, count[item]
    total += count[item]

    item="4-Def"
    printf "%s %d\n", item, count[item]
    total += count[item]

    item="5-Cau"
    printf "%s %d\n", item, count[item]
    total += count[item]

    item="6-Fix"
    printf "%s %d\n", item, count[item]
    total += count[item]

    item="7-Fix"
    printf "%s %d\n", item, count[item]
    total += count[item]

    item="8-Fix"
    printf "%s %d\n", item, count[item]
    total += count[item]

    item="9-Fix"
    printf "%s %d\n", item, count[item]
    total += count[item]

    item="10-Fi"
    printf "%s %d\n", item, count[item]
    total += count[item]

    item="11-Cl"
    printf "%s %d\n", item, count[item]
    total += count[item]

    printf "Total %d\n", total
}
