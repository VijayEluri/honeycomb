honeycomb.layout.datafrags = 5
honeycomb.layout.parityfrags = 2
honeycomb.oa.client.checksumalgorithm = ADLER32

honeycomb.layout.expansion_status = none
honeycomb.layout.expansion_nodeids = 109 110 111 112 113 114 115 116

# hard coded to eight because we have 32 disks and 4 disks per node, in order
# to make changes here we would have to make changes in the code Constants.java
# in order to have the right amount of disks 
honeycomb.cell.num_nodes = 8

# multi-cell emulation
honeycomb.silo.cellid = 1
