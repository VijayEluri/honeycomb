i pkginfo
i depend
i postinstall
i preremove
i hadb_setup.pl

f none /opt/honeycomb/bin/convert_to_hadbquery.sh=bin/convert_to_hadbquery.sh 0755 bin bin
f none /opt/honeycomb/bin/shutdown_hadb.sh=bin/shutdown_hadb.sh 0755 bin bin
f none /opt/honeycomb/bin/wipe_hadb.sh=bin/wipe_hadb.sh 0755 bin bin
f none /opt/honeycomb/bin/hadb_cfg_rewrite.pl=bin/hadb_cfg_rewrite.pl 0755 bin bin
f none /opt/honeycomb/lib/honeycomb-hadb.jar=lib/honeycomb-hadb.jar 0644 bin bin
f none /opt/honeycomb/lib/md_caches/honeycomb-hadbcache.jar=lib/md_caches/honeycomb-hadbcache.jar 0644 bin bin

