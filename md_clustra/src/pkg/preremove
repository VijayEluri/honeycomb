EDITOR=$PKG_INSTALL_ROOT/opt/honeycomb/sbin/node_config_editor.sh

$EDITOR del svc IO-SERVERS HADB
$EDITOR del svc MASTER-SERVERS HADBMaster

$EDITOR cp del /opt/honeycomb/lib/honeycomb-hadb.jar
$EDITOR cp del /opt/SUNWhadb/4/lib/hadbjdbc4.jar
$EDITOR cp del /opt/SUNWhadb/4/lib/hadbm.jar
$EDITOR cp del /opt/SUNWhadb/4/lib/hadbmgt.jar
$EDITOR cp del /opt/SUNWhadb/4/lib/jmx.jar
$EDITOR cp del /opt/SUNWhadb/4/lib/jmxremote.jar
$EDITOR cp del /opt/SUNWhadb/4/lib/jmxremote_optional.jar

exit 0
